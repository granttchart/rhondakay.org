{
  "name": "rhondakay",
  "version": "0.1.0",
  "description": "A memorial website to Rhonda Kay Smith, mother to Grant and Jessie.",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production",
    "dev:build": "NODE_ENV=development webpack --mode development",
    "start": "npm run move-assets && NODE_ENV=development webpack-dev-server --open",
    "move-assets": "mkdir -p dist && cp -r src/assets dist/",
    "move-index": "mkdir -p dist && cp -r src/index.html dist/",
    "deploy": "npm run move-assets && npm run move-index && npm run build && node scripts/deploy.js"
  },
  "author": "grantdotlocal",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "child_process": "^1.0.2",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "Explorer 11"
  ]
}
